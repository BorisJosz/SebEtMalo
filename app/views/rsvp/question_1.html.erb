<h1>Rsvp#question_1</h1>
<p>Find me in app/views/rsvp/question_1.html.erb</p>


<%= form_for :form, url: answer_1_path  do |f|%>
        <%= f.text_field :prenom %>
        <%= f.text_field :nom %>
        <%= f.text_field :invite %>
        <%= f.text_area :dance_question %>
        <%= f.check_box "RECEPTION", label: "reception"  %>
        <%= f.check_box "diner",  label: "diner"  %>
        <%= f.check_box "soiree", label: "soiree"  %>
        <%= f.submit %>
<% end %>